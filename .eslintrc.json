{
  "extends": "next/core-web-vitals",
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "jsx-quotes": [
      "error",
      "prefer-double"
    ],
    "no-multiple-empty-lines": [
      "error",
      {
        "max": 1,
        "maxEOF": 0,
        "maxBOF": 0
      }
    ]
  }
}